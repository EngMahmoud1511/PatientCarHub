@model PatientCarHub.EFModels.Models.StaticFiles

@{
    ViewData["Title"] = "UplodeExamenResult";
}

<h1>UplodeExamenResult</h1>

<h4>StaticFiles</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="UplodeExamenResult">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="File" class="control-label"></label>
                <input type="file" asp-for="File" onchange="LoadImge(event)" value="@Model?.File" />
                <img id="myimg" height="200" width="200" style="padding:20px 10px" value="~/DoctorIdentifiers/@Model?.File" />
                <span asp-validation-for="File" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="UploadeDate" class="control-label"></label>
                <input asp-for="UploadeDate" class="form-control" />
                <span asp-validation-for="UploadeDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
